  "scripts": {
    "presetup": "npm run lint:package",
    "setup": "npm install",
    "clean": "rimraf prod dist coverage storybook-static checkstyle.html",
    "prebuild": "npm run clean && mkdir dist && cp -r src/assets/* dist && npm run lint:package",
    "prebuild:dev": "npm run prebuild",
    "build:dev": "webpack-dev-server --mode development --inline --env._env=dev --env.brand=DARKNESS --env.device=desktop --open",
    "prebuild:all": "npm run prebuild",
    "build:all": "webpack-dev-server --mode development --inline --env._env=dev --env.brand=DARKNESS,REGULAR --env.device=desktop --open",
    "prerelease": "npm run prebuild && rimraf dist/index.html",
    "release": "webpack --mode production --env._env=prod --env.brand=DARKNESS,REGULAR --env.device=desktop",
    "prebuild:dist": "npm run clean && mkdir dist && cp -r src/assets/DARKNESS dist",
    "build:dist": "webpack --config webpack.config.js --env._env=dev",
    "prebuild:dist:all": "npm run prebuild",
    "build:dist:all": "webpack --config webpack.config.js --env._env=dev --env.brand=DARKNESS,REGULAR",
    "build": "npm run build:dist:all",
    "start": "node server.js",
    "preprtty": "npm run prtty:json",
    "prtty:json": "prettier --write --parser json .eslintrc .stylelintrc .prettierrc .babelrc src/config/**/*.json",
    "prtty": "prettier --write **/*.{js,jsx,ts,tsx,json,json5,css,less,scss,htm,html,md}",
    "prettier": "npm run prtty && npm run lint",
    "lint:package": "node scripts/lint-package-json.js package.json --allow-url",
    "preeslint:html": "npm run prelint",
    "_lint": "eslint --cache **/*.{js,jsx,ts,tsx}",
    "eslint": "npm run _lint -- --format codeframe",
    "eslint:html": "npm run _lint -- --format html > checkstyle.html",
    "eslint:fix": "npm run _lint -- --fix",
    "_slint": "stylelint --cache **/*.{htm,html,css,less,sass,scss,ss}",
    "lint:style": "npm run _slint",
    "lint:style:fix": "npm run _slint -- --fix && echo 'DANGEROUS stylelint --fix 9.9.0 does not respect stylelint-disable comments. Check that the changes are ok.'",
    "prelint": "npm run lint:package",
    "prelint:fix": "npm run prelint",
    "lint": "npm run eslint && npm run lint:style",
    "lint:fix": "npm run eslint:fix && npm run lint:style:fix",
    "test:unit": "jest --config specs/jest.config.json --forceExit",
    "test:debug": "TEST_DEBUG=1 npm run test:unit -- --debug",
    "test:watch": "npm run test:unit -- --watch",
    "test": "npm run test:unit -- --coverage",
    "test:build": "npm run lint && npm run test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@storybook/react": "4.0.12",
    "@storybook/addon-actions": "4.0.12",
    "@storybook/addon-links": "4.0.12",
    "@storybook/addons": "4.0.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prtty && npm run lint && npm run test:unit",
      "pre-push": "npm run test && npm run release"
    }
  }
}
