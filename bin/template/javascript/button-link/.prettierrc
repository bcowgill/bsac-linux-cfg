{
  "editorconfig": true,
  "printWidth": 100,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always"
  "overrides": [
    {
      "files": ["*.json", ".prettierrc"],
      "options": { 
        "parser": "json",
        "tabWidth": 2
      }
    }, {
      "files": ["*.js", "*.cjs", "*.mjs", "*.ts", "*.tsx", "*.cts", "*.mts"],
      "options": {
        "tabWidth": 4
      }
    }
  ]
}
