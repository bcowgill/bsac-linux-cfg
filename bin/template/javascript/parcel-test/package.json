{
  "name": "parcel-test",
  "version": "1.0.0",
  "description": "A test of the parcel bundler for browser and node using pnpm package manager",
  "main": "src/app/javascript.js",
  "engines": {
    "pnpm": "*",
    "node": ">=8.0.0"
  },
  "scripts": {
    "clean:all": "rimraf .cache/ dist/ dist-dev/ dist-node/",
    "clean": "rimraf dist/ dist-dev/ dist-node/",
    "prestart": "rimraf dist-dev/ && bash -c scripts/pre-browser.sh",
    "prestart:react": "npm run prestart",
    "prestart:vue": "npm run prestart",
    "prestart:typescript": "npm run prestart",
    "prestart:all": "npm run prestart",
    "prestart:benchmark": "npm run prestart",
    "prestart:esall": "npm run prestart",
    "browser": "browser.sh http://localhost:1234",
    "start": "parcel src/index.html -d dist-dev/",
    "start:react": "parcel src/react.html -d dist-dev/",
    "start:vue": "parcel src/vue.html -d dist-dev/",
    "start:typescript": "parcel src/typescript.html -d dist-dev/",
    "start:all": "parcel src/combined.html -d dist-dev/",
    "start:benchmark": "parcel src/benchmark.html -d dist-dev/",
    "start:esall": "parcel src/esall.html -d dist-dev/",
    "pretest": "bash -c scripts/pre-node.sh",
    "pretest:update": "npm run pretest",
    "pretest:debug": "npm run pretest",
    "precoverage": "npm run pretest",
    "test": "NODE_ENV=test jest --config jest.config.json --ci --notify --detectLeaks --detectOpenHandles --clearMocks --resetMocks --restoreMocks --forceExit",
    "test:update": "NODE_ENV=test jest --config jest.config.json --ci --notify --clearMocks --resetMocks --restoreMocks --forceExit --onlyFailures --updateSnapshot",
    "test:debug": "NODE_ENV=test jest --config jest.config.json --ci --notify --detectLeaks --detectOpenHandles --clearMocks --resetMocks --restoreMocks --forceExit --verbose --debug",
    "coverage": "NODE_ENV=test jest --config jest.config.json --ci --notify --detectLeaks --detectOpenHandles --clearMocks --resetMocks --restoreMocks --forceExit --coverage",
    "coverage:view": "browser.sh coverage/lcov-report/index.html",
    "prenode": "rimraf dist-node/ && bash -c scripts/pre-node.sh",
    "predebug": "npm run prenode",
    "prenode:typescript": "npm run prenode",
    "prebenchmark": "npm run prenode",
    "preesall": "npm run prenode",
    "predebug:typescript": "npm run prenode",
    "node": "BABEL_ENV=node parcel build src/app/javascript.js -d dist-node/ --detailed-report --log-level 3 --target node --no-minify && node dist-node/javascript.js",
    "debug": "BABEL_ENV=node parcel build src/app/javascript.js -d dist-node/ --target node --no-minify && node debug dist-node/javascript.js",
    "node:typescript": "BABEL_ENV=test parcel build src/app/typescript.ts -d dist-node/ --detailed-report --log-level 3 --target node --no-minify && node dist-node/typescript.js",
    "benchmark": "BABEL_ENV=node parcel build src/benchmark/bench-runner.js -d dist-node/ --detailed-report --log-level 3 --target node --no-minify && node dist-node/bench-runner.js",
    "esall": "BABEL_ENV=node parcel build src/lib/esall.js -d dist-node/ --detailed-report --log-level 3 --target node --no-minify && node dist-node/esall.js",
    "debug:typescript": "BABEL_ENV=node parcel build src/app/typescript.ts -d dist-node/ --target node --no-minify && node debug dist-node/typescript.js",
    "prebuild": "rimraf dist/ && bash -c scripts/pre-browser.sh",
    "prebuild:react": "npm run prebuild",
    "prebuild:vue": "npm run prebuild",
    "prebuild:typescript": "npm run prebuild",
    "prebuild:benchmark": "npm run prebuild",
    "prebuild:esall": "npm run prebuild",
    "prebuild:all": "npm run prebuild",
    "build": "NODE_ENV=production parcel build src/index.html",
    "build:react": "NODE_ENV=production parcel build src/react.html",
    "build:vue": "NODE_ENV=production parcel build src/vue.html",
    "build:typescript": "NODE_ENV=production parcel build src/typescript.html",
    "build:benchmark": "NODE_ENV=production parcel build src/benchmark.html",
    "postbuild:benchmark": "./scripts/post-build-benchmark.sh",
    "build:esall": "NODE_ENV=production parcel build src/esall.html",
    "build:all": "NODE_ENV=production parcel build src/combined.html"
  },
  "keywords": [
    "parcel-bundler",
    "pnpm",
    "yarn",
    "react",
    "vue",
    "typescript",
    "jest",
    "react-test-renderer",
    "enzyme",
    "test"
  ],
  "author": "Brent S.A. Cowgill",
  "license": "Unlicense",
  "devDependencies": {
    "@vue/component-compiler-utils": "2.0.0",
    "ansi-styles": "3.2.1",
    "babel-core": "6.26.3",
    "babel-jest": "23.2.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-exponentiation-operator": "6.24.1",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "benchmark": "2.1.4",
    "enzyme": "3.3.0",
    "fbjs": "0.8.17",
    "jest": "23.2.0",
    "jest-environment-jsdom": "23.2.0",
    "lodash": "4.17.10",
    "microtimer": "1.1.0",
    "node-sass": "4.9.0",
    "object-assign": "4.1.1",
    "parcel-bundler": "1.9.1",
    "platform": "1.3.5",
    "prettier": "1.15.2",
    "prop-types": "15.6.1",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-style-proptype": "3.2.1",
    "react-test-renderer": "16.4.1",
    "rimraf": "2.6.2",
    "typescript": "2.9.2",
    "vue": "2.5.16",
    "vue-hot-reload-api": "2.3.0",
    "vue-template-compiler": "2.5.16",
    "weak": "1.0.1"
  }
}
