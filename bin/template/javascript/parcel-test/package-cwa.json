{
  "name": "card-controls-nga",
  "version": "0.0.1",
  "description": "Personal Account Servicing - Card Controls",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf applications/*/dist applications/*/bundle-report.html coverage/ checkstyle-result.xml tests/acceptance/wdio/utilities/cucumberReport/ tests/acceptance/wdio/utilities/output/",
    "lint:one": "eslint --fix",
    "lint": "yarn lint:root && yarn lint:webpack && yarn lint:app",
    "lint:app": "eslint \"applications/**/*.{js,jsx}\" --fix",
    "lint:root": "eslint \"./*.js\" --fix",
    "lint:webpack": "eslint \"webpackPlugins/*.js\" --fix",
    "lint:bdd": "eslint \"tests/**/*.{js,jsx}\" --fix",
    "lint:report": "eslint \"{applications,webpackPlugins}/**/*.{js,jsx}\" -f checkstyle > checkstyle-result.xml",
    "test:json": "jest --coverage --verbose --json --outputFile=coverage/results.json",
    "test:updateSnapshot": "jest --updateSnapshot",
    "test:changed": "jest --verbose --onlyChanged",
    "test": "jest --coverage --verbose",
    "test:debug": "node --inspect --inspect-brk ./node_modules/.bin/jest",
    "test:summary": "jest",
    "test:all": "jest; echo DONE",
    "test:watch": "jest --verbose --watch",
    "prettier": "yarn prettier:root && yarn prettier:webpack && yarn prettier:app",
    "prettier:app": "prettier --single-quote --trailing-comma all --write \"applications/**/*.{js,jsx}\"",
    "prettier:root": "prettier --single-quote --trailing-comma none --write \"./*.js\"",
    "prettier:webpack": "prettier --single-quote --trailing-comma none --write \"webpackPlugins/*.js\"",
    "prettier:bdd": "prettier --single-quote --trailing-comma all --write \"tests/**/*.{js,jsx}\"",
    "build": "webpack --env.development",
    "release": "parallel-webpack -- --env.production",
    "release:debug": "node --inspect --inspect-brk ./node_modules/.bin/webpack --env.production",
    "start": "webpack-dev-server --env.start",
    "start:lloyds": "webpack-dev-server --env.start --env.lloyds",
    "start:halifax": "webpack-dev-server --env.start --env.halifax",
    "start:bos": "webpack-dev-server --env.start --env.bos",
    "start:nga": "webpack-dev-server --env.start --env.nga",
    "start:mca": "webpack-dev-server --env.start --env.mca",
    "start:mca:lloyds": "webpack-dev-server --env.start --env.lloyds --env.mca",
    "start:mca:halifax": "webpack-dev-server --env.start --env.halifax --env.mca",
    "start:mca:bos": "webpack-dev-server --env.start --env.bos --env.mca",
    "prepush": "/bin/bash -c ./hooks/git-is-clean.sh && yarn test && yarn release",
    "precommit": "lint-staged",
    "commitmsg": "/bin/bash -c ./hooks/commit-msg",
    "preparecommitmsg": "/bin/bash -c ./hooks/prepare-commit-msg",
    "runLocalBdd": "gulp runTestOnLocal",
    "bdd": "gulp runTestOnLocal"
  },
  "lint-staged": {
    "applications/**/*.js?(x)": [
      "test:summary",
      "prettier:app",
      "lint:app"
    ],
    "./*.js": [
      "prettier:root",
      "lint:root"
    ],
    "webpackPlugins/**/*.js": [
      "prettier:webpack",
      "lint:webpack"
    ]
  },
  "keywords": [],
  "author": {
    "name": "Shaun Wallace",
    "email": "swallace@sapient.com"
  },
  "license": "UNLICENSED",
  "engines": {
    "node": ">= 6",
    "npm": ">= 3"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "core-app-dynamics": "^1.0.0",
    "core-js": "^2.5.1",
    "core-nc-bridge": "^0.0.77",
    "cucumber-html-reporter": "^4.0.1",
    "es6-promise": "^4.1.1",
    "preact": "^8.2.5",
    "preact-compat": "^3.17.0",
    "query-string": "^5.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-preset-lloyds": "^2.0.5",
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.2.0",
    "core-acceptance-testing": "0.0.1",
    "core-cucumber-bdd": "1.0.6",
    "css-loader": "^0.28.7",
    "cucumber": "0.10.4",
    "enzyme": "^2.9.1",
    "eslint": "^4.8.0",
    "eslint-config-lloyds": "^1.1.9",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^1.1.4",
    "git-revision-webpack-plugin": "^2.5.1",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "4.0.0",
    "gulp-protractor-cucumber-html-report": "0.0.9",
    "gulp-webdriver": "2.0.2",
    "html-critical-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "js-yaml": "3.5.5",
    "lint-staged": "^4.2.3",
    "lodash-webpack-plugin": "^0.11.4",
    "node-sass": "^4.5.3",
    "parallel-webpack": "^2.2.0",
    "path": "0.12.7",
    "postcss-cssnext": "^3.0.2",
    "postcss-custom-properties": "^6.1.0",
    "postcss-loader": "^2.0.6",
    "postcss-smart-import": "^0.7.5",
    "prettier": "^1.7.3",
    "prop-types": "^15.6.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.6",
    "selenium-standalone": "6.4.1",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "wdio-cucumber-framework": "0.0.6",
    "wdio-sauce-service": "0.4.0",
    "webdriverio": "4.2.1",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0",
    "webpack-notifier": "^1.5.0",
    "yaml-merge": "0.0.2"
  }
}
 
