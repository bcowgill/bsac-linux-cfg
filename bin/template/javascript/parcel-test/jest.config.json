{
  "setupFiles": [
    "<rootDir>/src/test/jest-setup.js"
  ],
  "setupTestFrameworkScriptFile": "<rootDir>/src/test/jest-setup-test-framework.js",
  "roots": [
    "src/"
  ],
  "notify": true,
  "verbose": false,
  "collectCoverage": false,
  "coverageDirectory": "coverage",
  "coverageReporters": [
    "json",
    "lcov",
    "text",
    "text-summary"
  ],
  "collectCoverageFrom": [
    "src/**/**",
    "!**/index.js",
    "!**/test.js",
    "!**/*.spec.js",
    "!**/*.spec.jsx",
    "!**/*.snap",
    "!src/app/**",
    "!src/benchmark/**",
    "!src/facade/**",
    "!src/lib/**",
    "!src/test/jest*.js"
  ],
  "coveragePathIgnorePatterns": [
    "/(style|stub)\\.js$",
    "/dist/",
    "/dist-*/"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 25,
      "functions": 25,
      "lines": 25,
      "statements": 25
    }
  },
  "globals": {
    "webpack": {
      "title": "Application Accelerator Lloyds Brand Project",
      "brandName": "lloyds",
      "basePath": "/",
      "brandHome": "http://lloydsbank.com/personal.asp",
      "brandPrivacy": "http://www.lloydsbank.com/privacy.asp"
    }
  },
  "moduleFileExtensions": [
    "js",
    "jsx"
  ],
  "moduleNameMapper": {
  }
}
