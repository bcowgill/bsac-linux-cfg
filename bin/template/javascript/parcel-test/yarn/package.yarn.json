{
  "name": "parcel-test",
  "version": "1.0.0",
  "description": "A test of the parcel bundler for browser and node using pnpm package manager",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/ dist-dev/ dist-node/",
    "prestart": "rimraf dist-dev/",
    "prestart:react": "rimraf dist-dev/",
    "prestart:vue": "rimraf dist-dev/",
    "prestart:typescript": "rimraf dist-dev/",
    "prestart:all": "rimraf dist-dev/",
    "start": "parcel index.html -d dist-dev/",
    "start:react": "parcel react.html -d dist-dev/",
    "start:vue": "parcel vue.html -d dist-dev/",
    "start:typescript": "parcel typescript.html -d dist-dev/",
    "start:all": "parcel combined.html -d dist-dev/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prenode": "rimraf dist-node/",
    "node": "parcel build index.js -d dist-node/ --detailed-report --log-level 3 --target node --no-minify && node dist-node/index.js",
    "debug": "parcel build index.js -d dist-node/ --target node --no-minify && node debug dist-node/index.js",
    "node:typescript": "parcel build typescript.ts -d dist-node/ --detailed-report --log-level 3 --target node --no-minify && node dist-node/typescript.js",
    "debug:typescript": "parcel build typescript.ts -d dist-node/ --target node --no-minify && node debug dist-node/typescript.js",
    "prebuild": "rimraf dist/",
    "prebuild:react": "rimraf dist/",
    "prebuild:vue": "rimraf dist/",
    "prebuild:typescript": "rimraf dist/",
    "prebuild:all": "rimraf dist/",
    "build": "parcel build index.html",
    "build:react": "parcel build react.html",
    "build:vue": "parcel build vue.html",
    "build:typescript": "parcel build typescript.html",
    "build:all": "parcel build combined.html"
  },
  "keywords": [
    "parcel-bundler",
    "pnpm",
    "yarn",
    "react",
    "vue",
    "typescript",
    "test"
  ],
  "author": "Brent S.A. Cowgill",
  "license": "Unlicense",
  "devDependencies": {
    "@vue/component-compiler-utils": "^2.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "node-sass": "^4.9.0",
    "parcel-bundler": "^1.9.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "rimraf": "^2.6.2",
    "typescript": "^2.9.2",
    "vue": "^2.5.16",
    "vue-hot-reload-api": "^2.3.0",
    "vue-template-compiler": "^2.5.16"
  }
}
