  "scripts": {
    "setup": "npm install --no-optional --color-always",
    "nopostinstall": "npm install gulp --no-optional && gulp selenium:install && npm run check",
    "start": "gulp execute && npm run allure-report",
    "selenium": "gulp selenium:start",
    "saucelabs": "gulp saucelabs",
    "execute": "gulp execute",
    "execute:hub": "gulp execute-hub",
    "local-saucelabs": "gulp saucelabs --conf=grade1",
    "ci-saucelabs": "gulp saucelabs --conf=ci.grade1",
    "allure-report": "allure generate tests/acceptance/wdio/utilities/allure-results/ -o reports/allure_report",
    "test-pa11y": "gulp create-folders && pa11y-ci --config .pa11yci 2>&1 | tee reports/pa11y/results.log",
    "test-pa11y:bluemix": "gulp create-folders && pa11y-ci --config .pa11yci 2>&1 | tee reports/pa11y/results-bluemix.log",
    "test-sitespeed": "gulp sitespeed-test",
    "check": "./node_modules/.bin/npm-check -s --color-always || true",
    "cypress": "./node_modules/.bin/cypress open",
    "setup:dev": "npm install moment@2.24.0 cypress@4.5.0 gherkin@5.1.0 cypress-cucumber-preprocessor@1.11.0 cypress-plugin-retries@1.2.0 cypress-plugin-tab@1.0.3 cypress-image-snapshot@3.1.1 cypress-visual-regression@1.3.1 cypress-dark@1.7.14 @babel/core@7.0.0 babel-helpers@6.24.1 yamljs@0.3.0 prettier@1.16.4 husky@1.2.0; npm link; npm link cypress-cucumber-preprocessor; pushd cypress/fixtures; ./link.sh; popd",
    "cypress:start:dev": "pushd ../example-cwa && npm run dev",
    "cypress:dev": "cypress open --env 'environment=local,brand=generic'",
    "cypress:dev:brand": "cypress open --env 'environment=local,brand=brand'",
    "cypress:only": "cypress open --env 'environment=local,brand=generic,TAGS=@only'",
    "prtty": "prettier --write \"**/*.{js,jsx,ts,tsx,json,json5,css,less,scss,htm,html,md}\"",
    "prettier": "npm run prtty && npm run lint:fix",
    "lint:fix": "echo lint:fix coming soon...",
    "husky:pre-commit": "npm run prettier",
    "approve": "backstop approve",
    "test": "backstop test",
    "init": "backstop init",
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run husky:pre-commit"
    }
  },
  "precommit": []
