{
	"compileOnSave": true,
	"compilerOptions": {
		"module": "commonjs",
		"declaration": true,
		"sourceMap": true,
		"alwaysStrict": true,
		"noImplicitAny": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"strictNullChecks": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"forceConsistentCasingInFileNames": true
	}
}
